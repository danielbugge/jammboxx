<div class="cards_container">
  <% @jamms.each do |jam|  %>
    <div class="card">
      <div class="card-left">
        <div class="image">
          <p class= "jamm-address"><%= jam.address %></p>
          <p class= "jamm-date"><%= jam.date %></p>
        </div>
        <div class="card-right">
          <h3 class= "jamm-name"><%= jam.name %></h3>
          <h3 class= "jamm-genre"><%= jam.genre.name %></h3>
          <h3 class= "jamm-level"><%= jam.level %></h3>
          <%=  @instruments.where(jamm_id: jam.id).all.each do |user|  %>
            <p class= "jamm-user"> <%  user.id  %> </p>
          <% end %>
          <h3> Instruments </h3>
        </div>
      </div>
    <div class="card">
  <% end %>
</div>
